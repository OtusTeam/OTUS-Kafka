<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} %class - %msg%n</pattern>
    </encoder>
  </appender>

<!--  <logger name="org.apache.kafka" level="OFF"/>-->

  <root level="info">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
